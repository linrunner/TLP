local -a args reply
args=(
    # Command word.  Don't care what that is.
    /$'[^\0]#\0'/
)

_regex_words commands 'tlp-rdw command' \
    'disable:Disable RDW actions' \
    'enable:Enable RDW actions' \
    '--version:Print TLP version'
args+=("$reply[@]")

_regex_arguments _tlp-rdw "$args[@]"
_tlp-rdw "$@"
